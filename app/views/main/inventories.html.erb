<div class="container mt-4 mb-4">
    <h1>Inventories</h1>
    <table class="table">
    <thead>
        <tr>
            <th scope="col">Order</th>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
            <th scope="col">Dealer</th>
            <th scope="col">Time</th>
        </tr>
    </thead>
    <tbody>
        <% @data.each do |h| %>
            <tr>
                <th scope="row"><%= @order %></th>
                <td><%= h[1] %></td>
                <td><%= h[2] %></td>
                <td><%= h[0] %></td>
                <td><%= h[3] %></td>
            </tr>
            <% @order += 1 %>
        <% end %>
    </tbody>
    </table>
</div>
